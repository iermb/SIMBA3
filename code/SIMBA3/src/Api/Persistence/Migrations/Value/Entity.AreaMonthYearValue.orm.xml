<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="SIMBA3\Component\Domain\Value\Entity\AreaMonthYearValue" table="area_month_year_value">
        <id name="indicatorId" column="indicator_id" type="integer" nullable="false" />
        <id name="typeAreaCode" column="type_area_code" type="smallint" nullable="false" />
        <id name="areaCode" column="area_code" type="integer" nullable="false" />
        <id name="month" column="month" type="smallint" nullable="false" />
        <id name="year" column="year" type="smallint" nullable="false" />
        <field name="value" column="value" type="decimal" scale="4" precision="12" nullable="true" />
        <field name="isPublic" column="is_public" type="boolean" />
        <field name="noteValue" column="note_value" type="string" length="10" nullable="true" />

    </entity>

</doctrine-mapping>