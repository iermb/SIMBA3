<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="SIMBA3\Component\Domain\VariableIndicator\Entity\AreaIndicator" table="area_indicator">
        <id name="indicator" association-key="true" />
        <id name="typeAreaCode" type="integer" column="type_area_code" />
        <id name="areaCode" type="integer" column="area_code" />

        <many-to-one field="indicator" target-entity="SIMBA3\Component\Domain\Indicator\Entity\Indicator">
            <join-column name="indicator_id" referenced-column-name="id" />
        </many-to-one>
    </entity>

</doctrine-mapping>