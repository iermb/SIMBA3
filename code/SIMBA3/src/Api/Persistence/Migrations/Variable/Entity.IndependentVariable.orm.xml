<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="SIMBA3\Component\Domain\Variable\Entity\IndependentVariable" table="independent_variable">
        <id name="code" type="integer" column="code" />
        <id name="typeIndependentVariable" association-key="true" />
        <field name="name" column="name" type="string" length="100" nullable="false" />
        <many-to-one field="typeIndependentVariable" target-entity="SIMBA3\Component\Domain\Variable\Entity\TypeIndependentVariable">
            <join-column name="type_id" referenced-column-name="id" />
        </many-to-one>
    </entity>

</doctrine-mapping>