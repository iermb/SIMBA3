<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="SIMBA3\Component\Domain\Variable\Entity\TypeIndependentVariable" table="type_independent_variable">
        <id name="id" type="integer" column="id" />

        <unique-constraints>
            <unique-constraint columns="code,language" name="type_independent_variable_element" />
        </unique-constraints>

        <field name="code" column="code" type="integer" nullable="false" />
        <field name="name" column="name" type="string" length="100" nullable="false"/>
        <field name="language" column="language" type="string" length="2" nullable="false" />
    </entity>



</doctrine-mapping>